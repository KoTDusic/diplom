
@{
    ViewBag.Title = "Labs";
}
<body>
    <table id="main-table">
        <thead>
            <tr>
                <th>Дисциплина</th>
                <th>Студент</th>
                <th>Курс</th>
                <th>Группа</th>
                <th>Подгруппа</th>
                <th>Лабораторная</th>
                <th>Статус</th>
            </tr>
        </thead>
        <tbody>
            @{
                for (int i = 0; i < Model.Count; i++)
                {

                    <tr>
                        <td>@Model[i].disciplineName</td>
                        <td>@Model[i].studentName</td>
                        <td>@Model[i].coors</td>
                        <td>@Model[i].groupNumber</td>
                        <td>@Model[i].subgroopNumber</td>
                        <td>@Model[i].labName</td>
                        <td>@ElectronDecanat.Code.LabProgress.GetStatusText(Model[i].labStatus)</td>
                </tr>
                }
            }
        </tbody>
    </table>
    <script type="text/javascript">
        document.onload=new function () {
            $('#main-table').DataTable(russian_lang);
        };
    </script>
</body>
