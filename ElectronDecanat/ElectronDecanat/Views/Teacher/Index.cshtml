@using ElectronDecanat.Code;
@{
    ViewBag.Title = "Преподаватель";
}

<style type="text/css">
    table {
        margin-left: auto;
        margin-right: auto;
        margin-top: 5%;
        border: 1px solid black;
        border-collapse: collapse;
    }
    td {
        border: 1px solid black;
        padding: 10px;
    }
    th {
        border: 1px solid black;
        padding: 10px;
        font-size: 14px;
    }
</style>
<table>
<thead>
    <tr>
        <th>Преподаватель</th>
        <th>Специальность</th>
        <!--<th>Код специальности</th>-->
        <th>Дисциплина</th>
        <th>Курс</th>
        <!--<th>Год_поступления</th>-->
        <th>Группа</th>
        <th>Подгруппа</th>
    </tr>
</thead>
    <tbody>
        @{
            for (int i = 0; i < Model.Count; i++)
            {

                <tr>
                    <td>@Model[i].teacherNamel</td>
                    <td>@Model[i].specialityName</td>
                    <!--<td>Model[i].specialityCode</td>-->
                    <td>@Model[i].disciplineName</td>
                    <td>@Model[i].coors</td>
                    <!--<td>Model[i].year</td>-->
                    <td>@Model[i].groupNumber</td>
                    <td>@Model[i].subgroopNumber</td>
                </tr>
            }
        }
    </tbody>
</table>

